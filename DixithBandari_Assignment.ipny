    "#Specifing the file location to import the data(pdf file).\n",
    "pdf_file_path = \"HP_data.pdf\" \n",
    "\n",
    "#Initializing objects to determine birth month, date, and year.\n",
    "#My Date of Birth is 089/15/1997 or 15 of August, 1997.\n",
    "\n",
    "birth_month = 8  #birth month is August i.e 8\n",
    "birth_date = 15  #birth date is 15\n",
    "birth_year = 97  #birth year is 1997\n",
    "\n",
    "\n",
    "# Calculating the page number for given birth year.\n",
    "\n",
    "if birth_year >= 2000:\n",
    "    page_num_year = int(f\"1{str(birth_year)[2:]}\")  # Adding '1' in front of page numbers for years 2000 onwards\n",
    "else:\n",
    "    page_num_year = birth_year\n",
    "    \n",
    "# Calculating page numbers for given birth month and date.\n",
    "\n",
    "page_num_month = birth_month\n",
    "page_num_date = birth_date    \n",
    "\n",
    "#data containers to store the text after retreiving.\n",
    "\n",
    "text_data_file1 = \"\"\n",
    "text_data_file2 = \"\"\n",
    "\n",
    "#Extracting data from the PDF file.\n",
    "with open(pdf_file_path, 'rb') as pdf_file:\n",
    "    pdf_reader = PyPDF2.PdfReader(pdf_file)\n",
    "\n",
    "    # Extracting text from the first set of pages.\n",
    "    for page_num in range(page_num_month, page_num_month + 10):\n",
    "        page = pdf_reader.pages[page_num - 1] \n",
    "        text_data_file1 += page.extract_text()\n",
    "\n",
    "    # Extract text from the second set of pages.\n",
    "    for page_num in range(page_num_year, page_num_year + 10):\n",
    "        page = pdf_reader.pages[page_num - 1] \n",
    "        text_data_file2 += page.extract_text()\n",
    "\n",
    "#storing the extracted data into the files.        \n",
    "with open(\"file1.txt\", 'w', encoding='utf-8') as file1:\n",
    "    file1.write(text_data_file1)\n",
    "\n",
    "with open(\"file2.txt\", 'w', encoding='utf-8') as file2:\n",
    "    file2.write(text_data_file2)\n",
    "    \n",
    "    \n",
    "#Interpreting the data files after loading    \n",
    "print('Content in File1:')\n",
    "print(text_data_file1)\n",
    "print('Content in File2:')\n",
    "print(text_data_file2)\n",
    "    "
