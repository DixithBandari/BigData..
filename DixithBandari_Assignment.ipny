    "def is_english_word(word):\n",
    "    spell = SpellChecker()\n",
    "    # Checking if the word is in the English dictionary\n",
    "    return word in spell.word_frequency.dictionary\n",
    "\n",
    "# Processing and counting non-English words in file2.txt\n",
    "with open(\"file2.txt\", 'r', encoding='utf-8') as file2:\n",
    "    text_data_file2 = file2.read()\n",
    "    words = re.findall(r'\\b\\w+\\b', text_data_file2)\n",
    "    for word in words:\n",
    "        if not is_english_word(word):\n",
    "            non_english_words[word.lower()] += 1\n",
    "\n",
    "# Printing non-English words and their counts from file2.txt\n",
    "for word, count in non_english_words.items():\n",
    "    print(f\"Non-English Word: {word}, Count: {count}\")"
