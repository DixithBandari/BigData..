    "import re\n",
    "from spellchecker import SpellChecker\n",
    "from collections import defaultdict\n",
    "\n",
    "# Initializing a word count dictionary for MapReduce\n",
    "word_counts = defaultdict(int)\n",
    "\n",
    "# Function to process and count words\n",
    "def process_and_count_words(text):\n",
    "    words = re.findall(r'\\b\\w+\\b', text)\n",
    "\n",
    "    # Counting the occurrences of each word\n",
    "    for word in words:\n",
    "        word_counts[word.lower()] += 1\n",
    "\n",
    "# Processing and counting words in file1.txt\n",
    "with open(\"file1.txt\", 'r', encoding='utf-8') as file1:\n",
    "    text_data_file1 = file1.read()\n",
    "    process_and_count_words(text_data_file1)\n",
    "\n",
    "# Printing the words and their count results for file1.txt\n",
    "for word, count in word_counts.items():\n",
    "    print(f\"Word: {word}, Count: {count}\")\n"
   ]
